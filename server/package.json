{
  "name": "biphub-core",
  "version": "0.1.0",
  "description": "Biphub core project",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/biphub/biphub"
  },
  "author": "Jason Shin",
  "license": "MIT",
  "scripts": {
    "build:dev": "npx cross-env NODE_ENV=development gulp build",
    "start": "npx cross-env NODE_ENV=development nodemon -L --watch src --watch pods --watch gulpfile.babel.js --watch .env --exec \"gulp build && node\" ./dist/index.js",
    "test": "jest --forceExit",
    "xo:lint": "npx xo src/**/*.js ./manage.js ./migrate.js",
    "fix": "npx prettier --write \"./**/*.js\"",
    "sequelize": "npm run build:dev && npm run sequelize:clean && npm run sequelize:migrate && npm run sequelize:seed",
    "sequelize:clean": "node tools/sequelize/db_drop.js",
    "sequelize:migrate": "node ./migrate.js up",
    "sequelize:seed": "cross-env NODE_ENV=development node tools/sequelize/db_seed.js",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js"
  },
  "xo": {
    "space": true,
    "parser": "babel-eslint",
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "new-cap": 0,
      "camelcase": 0,
      "max-len": [
        "error",
        {
          "code": 80
        }
      ],
      "import/no-unassigned-import": 0,
      "max-nested-callbacks": [
        "error",
        5
      ],
      "object-curly-spacing": ["error", "always"],
      "comma-dangle": ["error", "always-multiline"],
      "space-before-function-paren": ["error", "never"],
      "capitalized-comments": 0
    }
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "**/test/**/*.test.(ts|js)"
    ],
    "testEnvironment": "node"
  },
  "dependencies": {
    "@types/graphql-type-json": "^0.1.1",
    "app-root-path": "^2.0.1",
    "async": "^2.5.0",
    "babel-eslint": "7",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-es2015-block-scoping": "^6.26.0",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "bcrypt-nodejs": "^0.0.3",
    "body-parser": "^1.18.2",
    "chalk": "^2.1.0",
    "change-case": "^3.0.1",
    "commander": "^2.12.2",
    "compression": "^1.7.0",
    "connect-mongo": "^1.3.2",
    "cors": "^2.8.4",
    "cross-env": "^5.1.3",
    "dotenv": "^2.0.0",
    "errorhandler": "^1.4.3",
    "express": "^4.15.4",
    "express-flash": "^0.0.2",
    "express-graphql": "^0.6.11",
    "express-session": "^1.15.5",
    "express-session-sequelize": "^1.1.0",
    "express-validator": "^3.2.1",
    "fbgraph": "^1.3.0",
    "fluture": "^7.1.2",
    "fs-extra": "^4.0.2",
    "graphql": "^0.11.7",
    "graphql-errors": "^2.1.0",
    "graphql-type-json": "^0.1.4",
    "gulp-changed": "^3.1.1",
    "gulp-eslint": "^4.0.2",
    "gulp-sourcemaps": "^2.6.1",
    "lusca": "^1.5.1",
    "mongoose": "^4.11.12",
    "morgan": "^1.7.0",
    "nodemailer": "^2.6.4",
    "npm-run": "^4.1.2",
    "passport": "0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "pg": "6",
    "pg-hstore": "^2.3.2",
    "pug": "^2.0.0-rc.4",
    "ramda": "^0.24.1",
    "ramda-fantasy": "^0.8.0",
    "relative": "^3.0.2",
    "request": "^2.82.0",
    "require-all": "^2.2.0",
    "sequelize": "^4.10.0",
    "slack": "^9.1.1",
    "source-map": "^0.6.1",
    "sqlite3": "^3.1.13",
    "string-template": "^1.0.0",
    "tracer": "^0.8.11",
    "umzug": "^2.1.0",
    "umzug-cli": "^2.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.0.0-beta.31",
    "concurrently": "^3.5.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-clean": "^0.3.2",
    "gulp-concat": "^2.6.1",
    "gulp-copy": "^1.0.1",
    "gulp-xo": "^0.16.0",
    "jest": "^19.0.2",
    "node-emoji": "^1.8.1",
    "node-sass": "^4.5.2",
    "nodemon": "^1.12.1",
    "pretty-error": "^2.1.1",
    "run-sequence": "^2.2.0",
    "shelljs": "^0.7.8",
    "source-map-support": "^0.4.18",
    "supertest": "^2.0.1",
    "xo": "^0.18.2"
  }
}
